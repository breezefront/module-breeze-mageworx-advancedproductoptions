<?xml version="1.0"?>
<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <body>
        <referenceBlock name="breeze.js">
            <arguments>
                <argument name="bundles" xsi:type="array">
                    <item name="product" xsi:type="array">
                        <item name="items" xsi:type="array">
                            <!-- Dynamic Options Base -->
                            <item name="dynamicOptions" xsi:type="array">
                                <item name="path" xsi:type="string">Swissup_BreezeMageworxAdvancedproductoptions/js/dynamicoptionbase/dynamicOptions</item>
                                <item name="import" xsi:type="array">
                                    <item name="dynamicOptionsDefaultCalculator" xsi:type="string">dynamicOptionsDefaultCalculator</item>
                                    <item name="price-utils" xsi:type="string">Magento_Catalog/js/price-utils</item>
                                    <item name="qTip" xsi:type="string">qTip</item>
                                    <item name="priceBox" xsi:type="string">priceBox</item>
                                    <item name="parent" xsi:type="string">MageWorx_DynamicOptionsBase/js/dynamicOptions</item>
                                </item>
                                <item name="load" xsi:type="array">
                                    <item name="onRequire" xsi:type="boolean">true</item>
                                </item>
                            </item>
                            <item name="dynamicOptionsDefaultCalculator" xsi:type="array">
                                <item name="path" xsi:type="string">Swissup_BreezeMageworxAdvancedproductoptions/js/dynamicoptionbase/calculator/default</item>
                                <item name="import" xsi:type="array">
                                    <item name="parent" xsi:type="string">MageWorx_DynamicOptionsBase/js/calculator/default</item>
                                </item>
                            </item>
                            <item name="dynamicOptionMinValueValidationRule" xsi:type="array">
                                <item name="path" xsi:type="string">Swissup_BreezeMageworxAdvancedproductoptions/js/dynamicoptionbase/validation/dynamicOptionMinValueValidationRule</item>
                                <item name="import" xsi:type="array">
                                    <item name="parent" xsi:type="string">MageWorx_DynamicOptionsBase/js/validation/dynamicOptionMinValueValidationRule</item>
                                </item>
                                <item name="load" xsi:type="array">
                                    <item name="onRequire" xsi:type="boolean">true</item>
                                </item>
                            </item>
                            <item name="dynamicOptionMaxValueValidationRule" xsi:type="array">
                                <item name="path" xsi:type="string">Swissup_BreezeMageworxAdvancedproductoptions/js/dynamicoptionbase/validation/dynamicOptionMaxValueValidationRule</item>
                                <item name="import" xsi:type="array">
                                    <item name="parent" xsi:type="string">MageWorx_DynamicOptionsBase/js/validation/dynamicOptionMaxValueValidationRule</item>
                                </item>
                                <item name="load" xsi:type="array">
                                    <item name="onRequire" xsi:type="boolean">true</item>
                                </item>
                            </item>
                            <item name="dynamicOptionStepValidationRule" xsi:type="array">
                                <item name="path" xsi:type="string">Swissup_BreezeMageworxAdvancedproductoptions/js/dynamicoptionbase/validation/dynamicOptionStepValidationRule</item>
                                <item name="import" xsi:type="array">
                                    <item name="parent" xsi:type="string">MageWorx_DynamicOptionsBase/js/validation/dynamicOptionStepValidationRule</item>
                                </item>
                                <item name="load" xsi:type="array">
                                    <item name="onRequire" xsi:type="boolean">true</item>
                                </item>
                            </item>

                            <!-- Option Advanced Pricing -->
                            <item name="optionAdvancedPricing" xsi:type="array">
                                <item name="path" xsi:type="string">Swissup_BreezeMageworxAdvancedproductoptions/js/optionadvancedpricing/advanced-pricing</item>
                                <item name="import" xsi:type="array">
                                    <item name="price-utils" xsi:type="string">Magento_Catalog/js/price-utils</item>
                                    <item name="priceBox" xsi:type="string">priceBox</item>
                                    <item name="parent" xsi:type="string">MageWorx_OptionAdvancedPricing/js/advanced-pricing</item>
                                </item>
                                <item name="load" xsi:type="array">
                                    <item name="onRequire" xsi:type="boolean">true</item>
                                </item>
                            </item>

                            <!-- Option Base -->
                            <item name="optionBase" xsi:type="array">
                                <item name="path" xsi:type="string">Swissup_BreezeMageworxAdvancedproductoptions/js/optionbase/catalog/product/base</item>
                                <item name="import" xsi:type="array">
                                    <item name="price-utils" xsi:type="string">Magento_Catalog/js/price-utils</item>
                                    <item name="qTip" xsi:type="string">qTip</item>
                                    <item name="parent" xsi:type="string">MageWorx_OptionBase/js/catalog/product/base</item>
                                </item>
                                <item name="load" xsi:type="array">
                                    <item name="onRequire" xsi:type="boolean">true</item>
                                </item>
                            </item>

                            <!-- Option Dependency -->
                            <item name="optionDependency" xsi:type="array">
                                <item name="path" xsi:type="string">Swissup_BreezeMageworxAdvancedproductoptions/js/optiondependency/dependency</item>
                                <item name="import" xsi:type="array">
                                    <item name="parent" xsi:type="string">MageWorx_OptionDependency/js/dependency</item>
                                </item>
                                <item name="load" xsi:type="array">
                                    <item name="onRequire" xsi:type="boolean">true</item>
                                </item>
                            </item>

                            <!-- Option Features -->
                            <item name="optionFeatures" xsi:type="array">
                                <item name="path" xsi:type="string">Swissup_BreezeMageworxAdvancedproductoptions/js/optionfeatures/catalog/product/features</item>
                                <item name="import" xsi:type="array">
                                    <item name="price-utils" xsi:type="string">Magento_Catalog/js/price-utils</item>
                                    <item name="priceBox" xsi:type="string">priceBox</item>
                                    <item name="qTip" xsi:type="string">qTip</item>
                                    <item name="parent" xsi:type="string">MageWorx_OptionFeatures/js/catalog/product/features</item>
                                </item>
                                <item name="load" xsi:type="array">
                                    <item name="onRequire" xsi:type="boolean">true</item>
                                </item>
                            </item>
                            <item name="optionFeaturesIsDefault" xsi:type="array">
                                <item name="path" xsi:type="string">Swissup_BreezeMageworxAdvancedproductoptions/js/optionfeatures/catalog/product/isDefault</item>
                                <item name="import" xsi:type="array">
                                    <item name="parent" xsi:type="string">MageWorx_OptionFeatures/js/catalog/product/isDefault</item>
                                </item>
                                <item name="load" xsi:type="array">
                                    <item name="onRequire" xsi:type="boolean">true</item>
                                </item>
                            </item>
                            <item name="selectionLimitValidationRule" xsi:type="array">
                                <item name="path" xsi:type="string">Swissup_BreezeMageworxAdvancedproductoptions/js/optionfeatures/selectionLimitValidationRule</item>
                                <item name="import" xsi:type="array">
                                    <item name="parent" xsi:type="string">MageWorx_OptionFeatures/js/selectionLimitValidationRule</item>
                                </item>
                                <item name="load" xsi:type="array">
                                    <item name="onRequire" xsi:type="boolean">true</item>
                                </item>
                            </item>

                            <item name="qTip" xsi:type="array">
                                <item name="path" xsi:type="string">Swissup_BreezeMageworxAdvancedproductoptions/js/optionfeatures/qtip</item>
                                <item name="load" xsi:type="array">
                                    <item name="autoload" xsi:type="boolean">true</item>
                                </item>
                            </item>

                            <item name="qTipWrapper" xsi:type="array">
                                <item name="path" xsi:type="string">Swissup_BreezeMageworxAdvancedproductoptions/js/optionfeatures/qTipWrapper</item>
                                <item name="import" xsi:type="array">
                                    <item name="qTip" xsi:type="string">qTip</item>
                                    <item name="parent" xsi:type="string">MageWorx_OptionFeatures/js/qTipWrapper</item>
                                </item>
                                <item name="load" xsi:type="array">
                                    <item name="onRequire" xsi:type="boolean">true</item>
                                </item>
                            </item>
                            <item name="optionAdditionalImages" xsi:type="array">
                                <item name="path" xsi:type="string">Swissup_BreezeMageworxAdvancedproductoptions/js/optionfeatures/swatches/additional</item>
                                <item name="import" xsi:type="array">
                                    <item name="mwImageReplacer" xsi:type="string">mwImageReplacer</item>
                                    <item name="parent" xsi:type="string">MageWorx_OptionFeatures/js/swatches/additional</item>
                                </item>
                                <item name="load" xsi:type="array">
                                    <item name="onRequire" xsi:type="boolean">true</item>
                                </item>
                            </item>
                            <item name="mwImageReplacer" xsi:type="array">
                                <item name="path" xsi:type="string">Swissup_BreezeMageworxAdvancedproductoptions/js/optionfeatures/swatches/imageReplacer</item>
                                <item name="import" xsi:type="array">
                                    <item name="parent" xsi:type="string">MageWorx_OptionFeatures/js/swatches/imageReplacer</item>
                                </item>
                                <item name="load" xsi:type="array">
                                    <item name="onRequire" xsi:type="boolean">true</item>
                                </item>
                            </item>

                            <!-- Option Inventory -->
                            <item name="optionInventory" xsi:type="array">
                                <item name="path" xsi:type="string">Swissup_BreezeMageworxAdvancedproductoptions/js/optioninventory/catalog/product/inventory</item>
                                <item name="import" xsi:type="array">
                                    <item name="parent" xsi:type="string">MageWorx_OptionInventory/js/catalog/product/inventory</item>
                                </item>
                                <item name="enabled" xsi:type="helper" helper="Swissup\Breeze\Helper\Config::isEnabled">
                                    <param name="path">mageworx_apo/optioninventory/enable</param>
                                </item>
                                <item name="load" xsi:type="array">
                                    <item name="onRequire" xsi:type="boolean">true</item>
                                </item>
                            </item>

                            <!-- Option Swatches -->
                            <item name="optionSwatches" xsi:type="array">
                                <item name="path" xsi:type="string">Swissup_BreezeMageworxAdvancedproductoptions/js/optionswatches/swatches</item>
                                <item name="import" xsi:type="array">
                                    <item name="price-utils" xsi:type="string">Magento_Catalog/js/price-utils</item>
                                    <item name="parent" xsi:type="string">MageWorx_OptionSwatches/js/swatches</item>
                                </item>
                                <item name="load" xsi:type="array">
                                    <item name="onRequire" xsi:type="boolean">true</item>
                                </item>
                            </item>
                        </item>
                    </item>
                </argument>
            </arguments>
        </referenceBlock>


        <referenceContainer name="breeze.container">
            <block class="Swissup\Breeze\Block\HtmlTemplate" name="breeze.MageWorx_OptionFeatures_template_option_gallery_dropdown_html" template="MageWorx_OptionFeatures::option/gallery/dropdown.html"/>
            <block class="Swissup\Breeze\Block\HtmlTemplate" name="breeze.MageWorx_OptionFeatures_template_option_gallery_radio_html" template="MageWorx_OptionFeatures::option/gallery/radio.html"/>
            <block class="Swissup\Breeze\Block\HtmlTemplate" name="breeze.MageWorx_OptionFeatures_template_option_gallery_checkbox_html" template="MageWorx_OptionFeatures::option/gallery/checkbox.html"/>
            <block class="Swissup\Breeze\Block\HtmlTemplate" name="breeze.MageWorx_OptionFeatures_template_option_gallery_empty_html" template="MageWorx_OptionFeatures::option/gallery/empty.html"/>
        </referenceContainer>
    </body>
</page>